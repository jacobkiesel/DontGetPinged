{
    "manifest_version": 3,
    "name": "DontGetPinged",
    "version": "1.0",
    "action":{
      "default_popup": "popup.html"
    },
    "background":{
      "service_worker": "popup.js"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "permissions": [
      "activeTab"
    ]
  }